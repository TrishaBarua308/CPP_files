#include <bits/stdc++.h>
using namespace std;
     
int main() {
    /**
    * Write your C++ code from here
    */
    string s;  cin>>s;
    char a[] = {
        '!', '@', '#', '$', '%', '^', '&', '*', '(', ')',
        '-', '_', '=', '+', '[', ']', '{', '}', '|', '\\',
        ':', ';', '\"', '\'', '<', '>', ',', '.', '?', '/',
        '`', '~'
    };
    int n = sizeof(a) / sizeof(a[0]) ;

    for(int i=0; i<s.length(); i++)
    {
        for(int j=0; j<n; j++)
        {
            if(s[i]==a[j])
            {
                s.erase(i,1);
            }
        }
    }

    cout<<s<<endl;

    return 0;
}
