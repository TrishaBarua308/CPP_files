#include <bits/stdc++.h>
using namespace std;

int main()
{
	int a[8];
	
	for(int i=0; i<8; i++)   cin>>a[i];
	bool flag = true;
	
	for(int i=0; i<7; i++)
	{
	    if(a[i]>a[i+1])
	    {
	       flag = false;
	    }
	}
	
	for(int i=0; i<8; i++)
	{
	    if( a[i]<100 || a[i]>675 || a[i]%25!=0 )
	    {
	       flag = false;
	    }
	}
	
	
	cout<<((flag)?"Yes\n":"No\n");

	return 0;
}
