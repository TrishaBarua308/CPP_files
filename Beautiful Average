#include <bits/stdc++.h>
#define optimize() ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
using namespace std;
int main() 
{
    optimize();
    int t;  cin>>t;
    
    while(t--)
    {
        int n;  cin>>n;
        int a[n];
        for(int i=0; i<n; i++) cin>>a[i];
        
        cout<<*max_element(a,a+n)<<endl;
    }

    //return 0;
}
