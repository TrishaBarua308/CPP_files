//mapleAndMultiplication 2139A
#include <bits/stdc++.h>
#define optimize() ios::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define endl "\n"
using namespace std;
int main()
{
    optimize();
    int t;    cin>>t;

    while(t--)
    {
        int a,b;    cin>>a>>b;

        if(a==b)         cout<<0<<endl;
        else if(a%b==0 || b%a==0)  cout<<1<<endl;
        else                  cout<<2<<endl;
    }

    return 0;
}
