#include <bits/stdc++.h>
using namespace std;
     
int main() 
{
    string s;      cin>>s;
    string ans = "";

    for(int i=0; i<s.length(); i+=2)
    {
        string part = s.substr(i,2);
        ans += part;

        if(i+2<s.length())
        {
            ans+='x';
        }
    }


    cout<<ans<<endl;

    return 0;
}
