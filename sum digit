#include <bits/stdc++.h>
using namespace std;
     
int main() {
    /**
    * Write your C++ code from here
    */
    int n;  cin>>n;

    int temp=n, sum=0;
    
    while(temp!=0)
    {
        int rem = temp%10;
        sum += rem;
        temp = temp/10;
    }

    cout<<((sum%2==0)?"Even":"Odd")<<endl;

    return 0;
}
