#include <bits/stdc++.h>
using namespace std;
     
int main() 
{
    int n, p, index=-1;  cin>>n;
    int a[n];

    for(int i=0; i<n; i++)  cin>>a[i];
    
    cin>>p;
    sort(a,a+n);

    for(int i=0; i<n; i++)
    {
        if(a[i]==p)
        {
            index = i;
        }
    }

    if(index!= -1) cout<<index<<endl;
    else cout<<"Element not found"<<endl;



    return 0;
}
