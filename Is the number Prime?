#include <bits/stdc++.h>
using namespace std;
     
int main() 
{
    int n;  cin>>n;
    int isprime=1;

    if(n<=1) isprime=0;
    else
    {
        for(int i=2; i*i<=n; i++)
        {
            if(n%i==0)
            {
                isprime=0;
                break;
            }
        }
    }

    if(isprime)  cout<<n<<" is a prime number."<<endl;
    else         cout<<n<<" is not a prime number."<<endl;

    return 0;
}
