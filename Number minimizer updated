#include <bits/stdc++.h>
using namespace std;
     
int main() {
    /**
    * Write your C++ code from here
    */
    string s;  cin>>s;
    int k;     cin>>k;
    
    int n=s.length() ;

    if(k>0 && s[0]!='1')
    {
        s[0]='1';
        k--;
    }

    for(int i=1; i<n && k>0; i++)
    {
        if(s[i]!='0')
        {
            s[i]='0';
            k--;
        }
    }

    cout<<"Min = "<<s<<endl;


    return 0;
}
