#include <bits/stdc++.h>
#define optimize() ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define endl '\n'
using namespace std;

int main() 
{
    string s;  cin>>s; 
    string temp = s;
    reverse(temp.begin(), temp.end());
    
    if(temp==s) cout<<"YES"<<endl;
    else cout<<"NO"<<endl;
    
    return 0;
}
