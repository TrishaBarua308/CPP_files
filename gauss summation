#include <bits/stdc++.h>
using namespace std;
#define ll long long
     
int main() {
    /**
    * Write your C++ code from here
    */

    ll n, count=1,sum=0;  cin>>n;

    for(ll i=1; i<=n/2; i++)
    {
        sum += i;
        if(sum>=n)
        {
            break;
        }
        else
        {
            count++;
        }
    }

    cout<<count<<endl;

    return 0;
}
