#include<bits/stdc++.h>
#define optimize() ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define endl '\n'
using namespace std;
int main()
{
    optimize();
    int t;  cin>>t;
    
    while(t--)
    {
        int a,b,c,d;   cin>>a>>b>>c>>d;
        
        if(a==b && b==c && c==d) cout<<"YES"<<endl;
        else cout<<"NO"<<endl;
    }
    
    return 0;
}
