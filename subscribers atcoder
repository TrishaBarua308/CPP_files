#include<bits/stdc++.h>
#define ll long long
#define endl "\n"
using namespace std;
int main()
{
    ll n;  cin>>n;
    
    ll a = pow(10,3);
    ll b = pow(10,4);
    ll c = pow(10,5);
    ll d = pow(10,6);
    ll e = pow(10,7);
    ll f = pow(10,8);
    ll g = pow(10,9);
    
    
    if(n<=a-1) cout<<n<<endl; //1
    else if(n>=a && n<=b-1) // 2
    {
        ll r = n%10;
        cout<<n-r<<endl;
    }
    
    else if(n>=b && n<=c) // 3
    {
        ll r = n%100;
        cout<<n-r<<endl;
    }
    
    else if(n>=c && n<=d) // 4
    {
        ll r = n%1000;
        cout<<n-r<<endl;
    }
    
    else if(n>=d && n<=e) // 5
    {
        ll r = n%10000;
        cout<<n-r<<endl;
    }
    
    else if(n>=e && n<=f) // 6
    {
        ll r = n%100000;
        cout<<n-r<<endl;
    }
    
    else if(n>=f && n<=g) // 5
    {
        ll r = n%1000000;
        cout<<n-r<<endl;
    }
    
    
    
    return 0;
}
