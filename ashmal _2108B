#include<bits/stdc++.h>
#define optimize() ios::sync_with_stdio(0); cin.tie(0);cout.tie(0);
#define endl '\n'
using namespace std;
int main()
{
    optimize();

    int t;   cin>>t;
    while(t--)
    {
        int n;   cin>>n;

        string s[n], ans;

        for(int i=0; i<n; i++)
        {
            cin>>s[i];
        }

        for(int i=0; i<n; i++)
        {
            ans = min(s[i]+ans,  ans+s[i]);
        }


        cout<<ans<<endl;
    }

}
