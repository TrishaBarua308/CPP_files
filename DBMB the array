#include <bits/stdc++.h>
#define optimize()  ios::sync_with_stdio(0);cin.tie(0); cout.tie(0);
#define endl "\n"
#define ll long long
using namespace std;

int main()
{
    optimize();

    int t;     cin >> t;



    while (t--)
    {
        int n, s, x;    cin >> n >> s >> x;
        int a[n];

        for (int i = 0; i < n; i++)  cin >> a[i];

        int sum = accumulate(a, a + n, 0);

        if (s >= sum && (s-sum)%x==0 )
            cout << "YES" << endl;
        else
            cout << "NO" << endl;
    }


    return 0;
}
