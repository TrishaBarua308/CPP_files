#include <bits/stdc++.h>
using namespace std;
#define ll long long
#define endl "\n"
int main()
{
    int t;   cin>>t;
    
    while(t--)
    {
        ll a[7];
        cin>>a[0]>>a[1]>>a[2]>>a[3]>>a[4]>>a[5]>>a[6];
        
        ll sum = accumulate(a, a+7, 0);
        
        cout<<sum<<" ";
    }

    return 0;
}
