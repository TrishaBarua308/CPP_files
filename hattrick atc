#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;  cin>>t;
	
	while(t--)
	{
	    char a[6];
	    cin>>a[0]>>a[1]>>a[2]>>a[3]>>a[4]>>a[5];
	    
	    bool flag=false;
	    
	    for(int i=0; i<6-2; i++)
	    {
	        if(a[i]=='W' && a[i+1]=='W' && a[i+2] == 'W')
	        {
	            flag=true;
	            break;
	        }
	    }
	    
	    cout<<(flag?"Yes\n":"No\n");
	}
	
	return 0;

}
