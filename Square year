#include<bits/stdc++.h>
#define optimize_io ios_base::sync_with_stdio(0); cin.tie(0); cout.tie(0);
#define ll long long
#define endl '\n'
using namespace std;
int main()
{
    int t;  cin>>t;

    while(t--)
    {
        int n; cin>>n;
        int a = n/100; //a
        int root = sqrt(n);
        int b = root - a;
        int c = pow((a+b), 2);
        if(c== n) cout<<a<<" "<<b<<endl;
        else cout<<"-1"<<endl;
    }
}
