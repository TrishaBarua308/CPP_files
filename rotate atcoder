#include<bits/stdc++.h>
using namespace std;
int main()
{
  string s;     cin>>s;
  
  int a = s[0] - '0';
  int b = s[1] - '0';
  int c = s[2] - '0';
  
  int s1 = (a*100) + (b*10) + c;
  int s2 = (b*100) + (c*10) + a;
  int s3 = (c*100) + (a*10) + b;
  
  cout<<s1+s2+s3<<endl;
  
  
  return 0;
   
}
