#include <bits/stdc++.h>
#define ll long long
using namespace std;
     
int main() {
    ll n, k, sum=0;  cin>>n>>k;
    ll a[n];

    for(ll i=0; i<n; i++)  cin>>a[i];

    sort(a,a+n);
    reverse(a, a+n);

    for(ll i=0; i<k; i++)
    {
        sum+= a[i];
    }


    cout<<sum<<endl;

    return 0;
}
