#include <bits/stdc++.h>
using namespace std;
     
int main() {
    /**
    * Write your C++ code from here
    */
    string s[5]={"zero","two","four","six","eight"};
    string input;   cin>>input;
    int ans=1;

    for(int i=0; i<5;  i++)
    {
        if(input==s[i])
        {
            ans=0;
        }
        
    }
    
    cout<<ans<<endl;

    return 0;
}
