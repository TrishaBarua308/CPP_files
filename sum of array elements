#include <bits/stdc++.h>
#define ll long long
using namespace std;
     
int main() 
{
    ll n;  cin>>n;
    ll a[n];

    for(ll i=0; i<n; i++)   cin>>a[i];
    sort(a, a+n);
    
    ll ans=0;

    for(int i=0; i<n; i++)
    {
        if(a[i]<a[n/2])
        {
            ans += a[i];
        }
    }

    cout<<ans;

    return 0;
}
