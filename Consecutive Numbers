#include <bits/stdc++.h>
using namespace std;
     
int main() {
    /**
    * Write your C++ code from here
    */
    int n;  cin>>n;

    int a[n];

    for(int i=0; i<n; i++) cin>>a[i];
    sort(a, a+n);

    for(int i=0; i<n-1; i++)
    {
        if(abs(a[i]-a[i+1])!=1)
        {
            cout<<"false"<<endl;
            return 0;
        }
    }
    cout<<"true"<<endl;

    return 0;
}
